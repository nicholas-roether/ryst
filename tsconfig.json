{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"lib": [
			"es7"
		],
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"isolatedModules": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
